<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="wxCalcFrame">
		<title>Simple wxWindows - BlahCorp</title>
		<centered>1</centered>
		<icon>/home/h4k0/Documentos/Proyectos/CodeBlocks/wxCalc/img/Professor-icon2.png</icon>
		<pos>-1,-1</pos>
		<size>424,424</size>
		<minsize>-1,-1</minsize>
		<maxsize>-1,-1</maxsize>
		<id_arg>0</id_arg>
		<object class="wxPanel" name="ID_PANEL1" variable="PanelCalc" member="yes">
			<pos>0,-32</pos>
			<size>384,352</size>
			<minsize>-1,-1</minsize>
			<maxsize>-1,-1</maxsize>
			<handler function="OnPanel1Paint" entry="EVT_PAINT" />
			<object class="wxButton" name="ID_BUTTON1" variable="ButtonAceptar" member="yes">
				<label>Aceptar</label>
				<pos>280,256</pos>
				<handler function="OnButtonAceptarClick" entry="EVT_BUTTON" />
			</object>
			<object class="wxButton" name="ID_BUTTON2" variable="ButtonCancelar" member="yes">
				<label>Cancelar</label>
				<pos>168,256</pos>
				<handler function="OnButtonCancelarClick" entry="EVT_BUTTON" />
			</object>
			<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="TextCtrlNumero1" member="yes">
				<value>Número 1</value>
				<pos>112,112</pos>
				<style>wxTE_CENTRE</style>
			</object>
			<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticTextSigno" member="yes">
				<label>+</label>
				<pos>224,128</pos>
				<size>16,16</size>
			</object>
			<object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="TextCtrlNumero2" member="yes">
				<value>Número 2</value>
				<pos>248,112</pos>
				<style>wxTE_CENTRE</style>
			</object>
			<object class="wxTextCtrl" name="ID_TEXTCTRL3" variable="TextCtrlResultado" member="yes">
				<value>Resultado</value>
				<pos>16,240</pos>
				<size>144,64</size>
				<style>wxTE_READONLY|wxTE_CENTRE</style>
			</object>
			<object class="wxRadioBox" name="ID_RADIOBOX1" variable="RadioBoxOperacion" member="yes">
				<label>Seleccione una operacion:</label>
				<content>
					<item>Sumar</item>
					<item>Restar</item>
					<item>Multiplicar</item>
					<item>Dividir</item>
					<item>Porcentaje</item>
					<item>Raiz Cuadrada</item>
					<item>Factorial</item>
					<item>Logaritmo</item>
				</content>
				<default>-1</default>
				<dimension>2</dimension>
				<pos>16,160</pos>
				<size>392,80</size>
				<maxsize>-1,-1</maxsize>
				<style>wxRA_SPECIFY_ROWS</style>
				<handler function="OnRadioBoxOperacionSelect" entry="EVT_RADIOBOX" />
			</object>
			<object class="wxStaticBitmap" name="ID_STATICBITMAP1" variable="StaticBitmap1" member="yes">
				<bitmap>/media/h4k0/cherryjazper/H4K0/gnu.png</bitmap>
				<pos>0,8</pos>
				<size>112,112</size>
			</object>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="idMenuGuardar" variable="MenuItem3" member="yes">
					<label>Guardar</label>
					<accel>Ctrl-G</accel>
					<help>Genere una copia de la operaciones generadas</help>
					<handler function="OnMenuGuardar" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
